<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="OneClass.MAUIApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:OneClass.MAUIApp"
    xmlns:views="clr-namespace:OneClass.MAUIApp.View"
    Shell.FlyoutBehavior="Disabled">

    <TabBar>
        <Tab Title="Home" Icon="splash.png">
            <ShellContent ContentTemplate="{DataTemplate local:MainPage}" />
        </Tab>
        <Tab Title="Hello" Icon="about.png" >
            <ShellContent ContentTemplate="{DataTemplate views:TodoPage}" />
        </Tab>
        <Tab Title="New" Icon="about.png"  >
            <ShellContent ContentTemplate="{DataTemplate views:JoinClassePage}" /> 
        </Tab>
    </TabBar>
    
    </Shell>
